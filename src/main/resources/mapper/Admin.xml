<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Admin">
    <select id="seleceadmin" resultType="Admin" parameterType="String">
        select * from admin where username = #{username}
    </select>
</mapper>