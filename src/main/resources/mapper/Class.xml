<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace是用于方法调用的映射,而不是实体类和表的映射,配置文件中设置的别名,和这个User没有关系 -->
<!-- 调用下面的标签 需要使用 namespace.id 来执行 -->
<mapper namespace="Class">

    <select id="selectid" resultType="Integer" parameterType="String">
        select id from  class where class.name = #{class_name}
    </select>


    <select id="selectids" resultType="Class" >
        select * from  class
    </select>


</mapper>